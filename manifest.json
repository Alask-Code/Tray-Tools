{
  "appName": "Tray Tools",
  "version": "1.1.0",
  "featureFlags": {
    "showLogs": true,
    "BrowserWindow": {
      "enabled": true,
      "options": {
        "frame": false,
        "transparent": true,
        "alwaysOnTop": true,
        "skipTaskbar": true,
        "webPreferences": {
          "nodeIntegration": true,
          "contextIsolation": false,
          "enableRemoteModule": true
        }
      }
    }
  }
}