{
  "appName": "Tray Tools",
  "version": "1.2.0",
  "developerMode": true,
  "openDevTools": false,
  "featureFlags": {
    "BrowserWindow": {
      "enabled": true,
      "options": {
        "frame": false,
        "transparent": true,
        "alwaysOnTop": true,
        "skipTaskbar": true,
        "width": 125,
        "height": 125,
        "webPreferences": {
          "nodeIntegration": true,
          "contextIsolation": false,
          "enableRemoteModule": true
        }
      }
    }
  }
}